{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["id", "displayName", "defaultGenes", "renderer"],
  "properties": {
    "id": {
      "type": "string"
    },
    "displayName": {
      "type": "string"
    },
    "renderer": {
      "type": "string"
    },
    "defaultGenes": {
      "type": "object",
      "properties": {
        "speed": {
          "type": "number"
        },
        "senseRadius": {
          "type": "number"
        },
        "hungerDrive": {
          "type": "number"
        },
        "rarityGene": {
          "type": "number"
        },
        "colorHue": {
          "type": "number"
        },
        "patternType": {
          "type": "string"
        }
      },
      "required": ["speed", "senseRadius", "hungerDrive", "rarityGene", "colorHue", "patternType"]
    }
  }
}
